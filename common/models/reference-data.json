        "LNG_REFERENCE_DATA_CATEGORY_CENTRE_NAME",
        "LNG_REFERENCE_DATA_CATEGORY_FOLLOWUP_GENERATION_TEAM_ASSIGNMENT_ALGORITHM",
        "LNG_REFERENCE_DATA_OUTBREAK_MAP_SERVER_TYPE"
      ]
    },
    "value": {
      "type": "string",
      "description": "Reference data item value; eg: Nurse",
      "required": true
    },
    "description": {
      "type": "string",
      "description": "Reference data item value description; eg: profession within the health care sector"
    },
    "iconId": {
      "type": "string"
    },
    "code": {
      "type": "string",
      "unique": true,
      "ignoreCase": true
    },
    "colorCode": {
      "type": "string"
    },
    "readOnly": {
      "type": "boolean",
      "readOnly": true,
      "default": false
    },
    "active": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "order": {
      "type": "number"
    },
    "isDefaultReferenceData": {
      "type": "boolean"
    },
    "isOutbreakTemplateReferenceData": {
      "type": "boolean"
    },
    "geoLocation": {
      "type": "customGeoPoint"
    },
    "isSystemWide": {
      "type": "boolean"
    },
    "allowedRefDataItems": {
      "type": "allowedRefData"
    }
  },
  "indexes": {
    "value_index": {
      "value": 1
    },
    "categoryId_index": {
      "categoryId": 1
    },
    "code_index": {
      "code": 1
    }
  },
  "validations": [],
  "relations": {},
