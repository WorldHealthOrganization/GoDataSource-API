{
  "name": "case",
  "base": "extendedPersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mongodb": {
    "collection": "person"
  },
  "scope": {
    "where": {
      "type": "case"
    }
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true,
      "default": "case"
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "middleName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "dob": {
      "type": "date"
    },
    "age": {
      "type": "number"
    },
    "gender": {
      "type": "string",
      "required": true
    },
    "phoneNumber": {
      "type": "string"
    },
    "occupation": {
      "type": "string"
    },
    "documentType": {
      "type": "string"
    },
    "documentNumber": {
      "type": "string"
    },
    "dateOfInfection": {
      "type": "date"
    },
    "dateOfOnset": {
      "type": "date"
    },
    "dateBecomeCase": {
      "type": "date"
    },
    "dateOfOutcome": {
      "type": "date"
    },
    "isolationDates": ["date"],
    "hospitalizationDates": ["date"],
    "incubationDates": ["date"],
    "deceased": {
      "type": "boolean",
      "default": false
    },
    "dateDeceased": {
      "type": "date"
    },
    "locationId": {
      "type": "string"
    },
    "address": {
      "type": "address"
    },
    "geoLocation": {
      "type": "geopoint"
    },
    "classification": {
      "type": "string",
      "required": true
    },
    "riskLevel": {
      "type": "string",
      "required": true
    },
    "riskReason": {
      "type": "string"
    },
    "fillGeoLocation": {
      "type": "geopoint"
    }
  },
  "validations": [],
  "relations": {
    "labResults": {
      "type": "hasMany",
      "model": "labResult",
      "foreignKey": "personId"
    },
    "relationships": {
      "type": "hasMany",
      "model": "relationship",
      "foreignKey": "personId"
    }
  },
  "acls": [],
  "methods": {}
}
