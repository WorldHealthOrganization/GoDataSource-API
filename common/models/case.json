{
  "name": "case",
  "base": "person",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "CascadeDelete": {
      "relations": [
        "relationships",
        "labResults"
      ]
    }
  },
  "hidden": [
    "type"
  ],
  "scope": {
    "where": {
      "type": "case"
    }
  },
  "properties": {
    "type": {
      "type": "string",
      "required": true,
      "default": "case",
      "readOnly": true
    },
    "dateOfInfection": {
      "type": "date"
    },
    "dateOfOnset": {
      "type": "date"
    },
    "isDateOfOnsetApproximate": {
      "type": "boolean"
    },
    "dateBecomeCase": {
      "type": "date"
    },
    "outcomeId": {
      "type": "string"
    },
    "dateOfOutcome": {
      "type": "date"
    },
    "isolationDates": ["dateRange"],
    "hospitalizationDates": ["dateRange"],
    "incubationDates": ["dateRange"],
    "deceased": {
      "type": "boolean",
      "default": false
    },
    "dateDeceased": {
      "type": "date"
    },
    "classification": {
      "type": "string",
      "required": true
    },
    "riskLevel": {
      "type": "string"
    },
    "riskReason": {
      "type": "string"
    },
    "transferRefused": {
      "type": "boolean",
      "default": false
    },
    "questionnaireAnswers": {
      "type": "object"
    }
  },
  "excludeBaseProperties": [
    "name",
    "description"
  ],
  "validations": [],
  "relations": {
    "labResults": {
      "type": "hasMany",
      "model": "labResult",
      "foreignKey": "personId"
    }
  },
  "acls": [],
  "methods": {}
}
