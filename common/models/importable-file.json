                "<fileHeader>": {
                  "type": "string",
                  "description": "Model property"
                }
              }
            },
            "modelProperties": {
              "type": {
                "<modelProperty>": [{
                  "type": "string",
                  "description": "Model property label"
                }]
              }
            },
            "modelPropertyValues": {
              "type": {
                "<modelProperty>": [{
                  "type": "string",
                  "description": "Available values for a model property"
                }]
              }
            },
            "distinctFileColumnValues": {
              "type": {
                "<fileHeader>": [{
                  "type": "string",
                  "description": "Distinct values for column"
                }]
              }
            }
          },
          "root": true,
          "required": true
        }
      ],
      "description": "Upload a file and get file headers",
      "http": {
        "verb": "post",
        "path": "/"
      }
    },
    "getJsonById": {
      "accepts": [
        {
          "arg": "id",
          "type": "string",
          "required": true
        }
      ],
      "returns": [
        {
          "arg": "json",
          "type": ["any"],
          "required": true,
          "root": true
        }
      ],
      "description": "Retrieve JSON version of an uploaded file",
      "http": {
        "verb": "get",
        "path": "/:id/json"
      }
    }
  }
