      "type": "string",
      "description": "URL of the server with which the client is synced. Present in the entries for Client instances"
    },
    "syncClientId": {
      "type": "string",
      "description": "Client ID which is synced with the server. Present in the entries for Server instances"
    },
    "syncProcessStartDate": {
      "type": "date",
      "required": true
    },
    "syncProcessCompletionDate": {
      "type": "date"
    },
    "syncStatus": {
      "type": "string",
      "enum": [
        "LNG_SYNC_STATUS_IN_PROGRESS",
        "LNG_SYNC_STATUS_SUCCESS",
        "LNG_SYNC_STATUS_FAILED"
      ],
      "required": true
    },
    "syncOutbreakIDs": {
      "type": [
        "string"
      ],
      "description": "IDs of the synced outbreaks"
    },
    "syncInformationStartDate": {
      "type": "date",
