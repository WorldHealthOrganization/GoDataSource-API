{
  "name": "location",
  "base": "extendedPersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "synonyms": {
      "type": "array"
    },
    "active": {
      "type": "boolean",
      "required": true
    },
    "populationDensity": {
      "type": "number"
    },
    "parentLocationId": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "parentLocation": {
      "type": "belongsTo",
      "model": "location",
      "foreignKey": "parentLocationId"
    },
    "teams": {
      "type": "hasMany",
      "model": "team",
      "foreignKey": "locationId"
    },
    "events": {
      "type": "hasMany",
      "model": "event",
      "foreignKey": "locationId"
    }
  },
  "acls": [],
  "methods": {}
}
