{
  "name": "contact",
  "base": "person",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "CascadeDelete": {
      "relations": [
        {
          "relation": "relationships",
          "operations": {
            "restore": {
              "where": {
                "deletedByParent": ":FK_UUID"
              },
              "set": {
                "deletedByParent": null
              }
            },
            "delete": {
              "where": {
                "deletedByParent": {
                  "eq": null
                }
              },
              "set": {
                "deletedByParent": ":FK_UUID"
              }
            }
          }
        },
        {
          "relation": "followUps",
          "operations": {
            "restore": {
              "where": {
                "deletedByParent": ":FK_UUID"
              },
              "set": {
                "deletedByParent": null
              }
            },
            "delete": {
              "where": {
                "deletedByParent": {
                  "eq": null
                }
              },
              "set": {
                "deletedByParent": ":FK_UUID"
              }
            }
          }
        }
      ]
    }
  },
  "hidden": [
    "type"
  ],
  "scope": {
    "where": {
      "type": "LNG_REFERENCE_DATA_CATEGORY_PERSON_TYPE_CONTACT"
    },
    "order": ["lastName ASC", "firstName ASC"]
  },
  "properties": {
    "type": {
      "type": "string",
      "default": "LNG_REFERENCE_DATA_CATEGORY_PERSON_TYPE_CONTACT",
      "required": true,
      "readOnly": true
    },
    "firstName": {
      "type": "string",
      "required": true
    },
    "gender": {
      "type": "string"
    },
    "riskLevel": {
      "type": "string"
    },
    "riskReason": {
      "type": "string"
    },
    "outcomeId": {
      "type": "string"
    },
    "safeBurial": {
      "type": "boolean",
      "default": false
    },
    "wasCase": {
      "type": "boolean",
      "default": false
    },
    "active": {
      "type": "boolean",
      "description": "A contact is active if at least one of the cases related to it has non-discarded classification",
      "default": true
    },
    "vaccinesReceived": {
      "type": [
        "vaccine"
      ]
    },
    "pregnancyStatus": {
      "type": "string"
    }
  },
  "excludeBaseProperties": [
    "name",
    "description",
    "dateOfInfection",
    "dateOfOnset",
    "dateBecomeCase",
    "dateRanges",
    "classificationHistory",
    "date"
  ],
  "validations": [],
  "relations": {
    "followUps": {
      "type": "hasMany",
      "model": "followUp",
      "foreignKey": "personId"
    },
    "labResults": {
      "type": "hasMany",
      "model": "labResult",
      "foreignKey": "personId"
    }
  },
  "acls": [],
  "methods": {}
}
