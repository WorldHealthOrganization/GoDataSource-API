{
  "initial:before": {
    "./middleware/sanitizeLimit.js": {}
  },
  "initial": {
    "compression": {},
    "helmet#xssFilter": {
      "params": {
        "setOnOldIE": true
      }
    },
    "helmet#frameguard": {
      "params": [
        "deny"
      ]
    },
    "helmet#hsts": {
      "params": {
        "maxAge": 63072000,
        "includeSubDomains": true,
        "preload": true
      }
    },
    "helmet#hidePoweredBy": {},
    "helmet#ieNoOpen": {},
    "helmet#noSniff": {}
  },
  "session": {},
  "auth": {
    "loopback#token": {
      "params": { "accessTokenId": "accessToken", "model": "accessToken" }
    }
  },
  "parse": {
    "body-parser#json": {
      "params": {
        "limit": "10mb"
      }
    },
    "body-parser#urlencoded": {
      "params": {
        "limit": "10mb",
        "extended": true
      }
    }
  },
  "routes": {
    "loopback#rest": {
      "paths": [
        "${restApiRoot}"
      ]
    }
  },
  "files": {},
  "final": {
    "loopback#urlNotFound": {}
  },
  "final:after": {
    "./middleware/passwordResetError": {},
    "./middleware/authenticationError": {},
    "./middleware/mongoDBError": {},
    "./middleware/internalError": {},
    "strong-error-handler": {
      "params": {
        "log": true,
        "safeFields": ["code", "message", "details"]
      }
    }
  }
}
