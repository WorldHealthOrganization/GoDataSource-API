{
  "name": "address",
  "base": "Model",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "RemovePropertiesFromDefinition": true
  },
  "hidden": [
    "id"
  ],
  "properties": {
    "typeId": {
      "type": "string",
      "required": true,
      "default": "LNG_REFERENCE_DATA_CATEGORY_ADDRESS_TYPE_USUAL_PLACE_OF_RESIDENCE"
    },
    "country": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "addressLine1": {
      "type": "string"
    },
    "addressLine2": {
      "type": "string"
    },
    "postalCode": {
      "type": "string"
    },
    "locationId": {
      "type": "string"
    },
    "geoLocation": {
      "required": false,
      "type": {
        "lat": {
          "type": "number",
          "required": false
        },
        "lng": {
          "type": "number",
          "required": false
        }
      }
    },
    "date": {
      "type": "date",
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {},
  "acls": [],
  "methods": {}
}
